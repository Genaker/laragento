{
    "name": "genaker/laragento",
    "description": "Use Magento backend with Laravel or any PHP framework",
    "homepage": "http://github.com/Genaker/laragento",
    "license": "MIT",
    
    "require": {
        "php": ">=7.3",
        "illuminate/database": "dev-8.x",
        "illuminate/filesystem": "^8.0",
        "illuminate/support": "^8.0",
        "fakerphp/faker": "^1.10",
        "hautelook/phpass": "0.3.*",
        "thunderer/shortcode": "^0.7.3",
        "kirschbaum-development/eloquent-power-joins": "^2.4"
    },
    "require-dev": {
        "laravel/legacy-factories": "^1.0",
        "orchestra/testbench": "^6.0",
        "symfony/thanks": "^1.0",
        "mockery/mockery": "^1.0",
        "dms/phpunit-arraysubset-asserts": "^0.1.0"
    },
    "autoload": {
        "psr-4": {
            "Laragento\\": "src/"
        }
    }
}
